			<Component ID="Landing_Pulse">
				<UseTemplate Name="ASOBO_LIGHTING_Light_Base_Template">
					<IE_NAME>LANDING_Lights</IE_NAME>
					<INTERACTION_TYPE>Switch</INTERACTION_TYPE>
					<LIGHT_TYPE>LANDING</LIGHT_TYPE>
					<NODE_ID>LIGHTING_Switch_Light_Landing</NODE_ID>
					<ANIM_NAME>switch_wig_wag</ANIM_NAME>
					<PART_ID>LIGHTING_Switch_Light_Landing</PART_ID>
					<TT_DESCRIPTION_ID>@TT_Package.LIGHTING_SWITCH_LIGHT_LANDING_ACTION_SET</TT_DESCRIPTION_ID>
					<TOOLTIP_TITLE>@TT_Package.LIGHTING_SWITCH_LIGHT_LANDING_TITLE</TOOLTIP_TITLE>
					<TT_VALUE>(I:LANDING_LIGHTS_SWITCH_ON) @TT_OnOff_Cond</TT_VALUE>
					<TT_VALUE_IS_DYNAMIC>True</TT_VALUE_IS_DYNAMIC>
					<SET_STATE_ON>1 (&gt;I:LANDING_LIGHTS_SWITCH_ON)</SET_STATE_ON>
					<SET_STATE_OFF>0 (&gt;I:LANDING_LIGHTS_SWITCH_ON)</SET_STATE_OFF>
					<GET_STATE_EXTERNAL>(I:LANDING_LIGHTS_SWITCH_ON) sp0</GET_STATE_EXTERNAL>
					<OVERRIDE_ANIM_CODE>(I:LANDING_LIGHTS_SWITCH_ON) 100 *</OVERRIDE_ANIM_CODE>
					<ANIM_CODE>(I:LANDING_LIGHTS_SWITCH_ON) 100 *</ANIM_CODE>
				</UseTemplate>
				<UseTemplate Name="ASOBO_LIGHTING_Switch_Potentiometer_Template">
					<NODE_ID>LIGHTING_Switch_Light_Pulse</NODE_ID>			
					<ANIM_NAME>switch_wig_wag</ANIM_NAME>
					<PART_ID>LIGHTING_Push_Pulse</PART_ID>
					<TOOLTIP_TITLE>@TT_Package.LIGHTING_SWITCH_LIGHT_PULSE_TITLE</TOOLTIP_TITLE>
					<TT_DESCRIPTION_ID>@TT_Package.LIGHTING_SWITCH_LIGHT_PULSE_ACTION</TT_DESCRIPTION_ID>
					<INVERT_ANIM>True</INVERT_ANIM>
					<POTENTIOMETER>7</POTENTIOMETER>
					<ID>7</ID>
				</UseTemplate>
				<UseTemplate Name="ASOBO_GT_Update">
					<FREQUENCY>2</FREQUENCY>
						<UPDATE_CODE>
						(I:LANDING_LIGHTS_SWITCH_ON) 1 == if{
							(L:LIGHTING_POTENTIOMETER_7) 100 == if{
								(&gt;K:LANDING_LIGHTS_TOGGLE)
							}
							els{
								(&gt;K:LANDING_LIGHTS_ON)
							}
						}
						els{
							(&gt;K:LANDING_LIGHTS_OFF)
						}
						</UPDATE_CODE>
				</UseTemplate>
				<UseInputEvent ID="PROCEDURE">
					<INPUT_EVENT_ID_SOURCE>PROCEDURE</INPUT_EVENT_ID_SOURCE>
					<IE_NAME>Toggle_Landing_Light</IE_NAME>
					<SET_ARG_COUNT>1</SET_ARG_COUNT>
						<BINDING_SET_0>L0</BINDING_SET_0>
						<BINDING_SET_0_PARAM_0>1</BINDING_SET_0_PARAM_0>
						<BINDING_SET_0_EVENT_ID>LANDING_LIGHTS_TOGGLE</BINDING_SET_0_EVENT_ID>
						<BINDING_SET_0_EVENT_ID_ONLY>True</BINDING_SET_0_EVENT_ID_ONLY>
						<BINDING_SET_1>L1</BINDING_SET_1>
						<BINDING_SET_1_PARAM_0>2</BINDING_SET_1_PARAM_0>
						<BINDING_SET_1_EVENT_ID>ALL_LIGHTS_TOGGLE</BINDING_SET_1_EVENT_ID>
						<BINDING_SET_1_EVENT_ID_ONLY>True</BINDING_SET_1_EVENT_ID_ONLY>
					<INC_CODE>1 (&gt;B:#INPUT_EVENT_ID_SOURCE#_#IE_NAME#_Set)</INC_CODE>
					<DEC_CODE>0 (&gt;B:#INPUT_EVENT_ID_SOURCE#_#IE_NAME#_Set)</DEC_CODE>
					<SET_CODE>
						p0 2 == if{
							(&gt;K:ALL_LIGHTS_TOGGLE)
							(A:LIGHT STROBE:1, Bool) (I:LANDING_LIGHTS_SWITCH_ON, Bool) == if{ quit }
						}
						(I:LANDING_LIGHTS_SWITCH_ON) 1 == if{ 0 (&gt;I:LANDING_LIGHTS_SWITCH_ON) }
						els{ 1 (&gt;I:LANDING_LIGHTS_SWITCH_ON) }
					</SET_CODE>
					<TT_DESCRIPTION>@TT_Package.GT_STATE_NA</TT_DESCRIPTION> <TT_DESCRIPTION_IS_DYNAMIC>False</TT_DESCRIPTION_IS_DYNAMIC>
					<TT_VALUE>@TT_Package.GT_STATE_NA</TT_VALUE> <TT_VALUE_IS_DYNAMIC>False</TT_VALUE_IS_DYNAMIC>
					<TT_ICON>PUSH</TT_ICON> <TT_INTERACTION>PRIMARY_DOWN</TT_INTERACTION> <TT_INTERACTION_LOCKABLE>LOCK</TT_INTERACTION_LOCKABLE>
				</UseInputEvent>
			</Component>
		</Component>
		
